<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hr.mapper.ConfigQuestionSecondKindMapper">
	<select id="select" resultType="com.hr.pojo.ConfigQuestionSecondKind">
    	select * from config_question_second_kind
	</select>
	<select id="selectByQfkId" parameterType="String" resultType="com.hr.pojo.ConfigQuestionSecondKind">
		select * from config_question_second_kind where qfk_id=#{qfkId}
	</select>
	<select id="selectByFirstKindId" parameterType="String" resultType="com.hr.pojo.ConfigQuestionSecondKind">
		select * from config_question_second_kind where first_kind_id=#{firstKindId}
	</select>
	<insert id="insert" parameterType="com.hr.pojo.ConfigQuestionSecondKind">
		insert into config_question_second_kind values (#{firstKindId},#{firstKindName},#{secondKindId},#{secondKindName})
	</insert>
	<update id="update" parameterType="com.hr.pojo.ConfigQuestionSecondKind">
		update config_question_second_kind set first_kind_id=#{firstKindId},first_kind_name=#{firstKindName},second_kind_id=#{secondKindId},second_kind_name=#{secondKindName} where qfk_id=#{qfkId}
	</update>
	<delete id="delete" parameterType="String">
		delete from config_question_second_kind where qfk_id=#{qfkId}
	</delete>
</mapper>